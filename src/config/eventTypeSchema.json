        { "id": "voting_weight", "type": "number", "label": "Public Vote Weight (%)", "default": 50, "conditional": "voting_enabled" },
        { "id": "jury_members", "type": "person-list", "label": "Jury Members", "showPublic": true },
        { "id": "dress_code", "type": "select", "label": "Dress Code", "options": ["formal", "semi_formal", "business", "smart_casual", "casual"], "showPublic": true },
        { "id": "ceremony_host", "type": "person-list", "label": "Ceremony Host/MC", "showPublic": true },
