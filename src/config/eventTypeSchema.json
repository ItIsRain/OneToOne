{
  "version": "1.0.0",
  "description": "OneToOne Event Management - Dynamic Event Type Schema for UAE Startup Ecosystem",

  "sharedFields": [
    "title", "description", "start_date", "end_date", "start_time", "end_time",
    "timezone", "is_virtual", "location", "virtual_platform", "virtual_link",
    "color", "cover_image", "max_attendees", "registration_required",
    "registration_deadline", "is_public", "is_published", "tags"
  ],

  "eventTypes": {
    "general": {
      "label": "General Event",
      "icon": "calendar",
      "color": "#6b7280",
      "description": "A standard event with basic settings",
      "defaultDuration": 2,
      "capacityDefault": 50,
      "fields": [
        { "id": "notes", "type": "textarea", "label": "Internal Notes", "adminOnly": true },
        { "id": "action_items", "type": "checklist", "label": "Action Items" },
        { "id": "attachments", "type": "file", "label": "Attachments" },
        { "id": "follow_up_date", "type": "date", "label": "Follow-up Date" }
      ],
      "flows": ["registration"],
      "integrations": ["calendar", "email"]
    },

    "meeting": {
      "label": "Meeting",
      "icon": "users",
      "color": "#3b82f6",
      "description": "Business meetings with agenda and attendees",
      "defaultDuration": 1,
      "capacityDefault": 20,
      "tabs": ["basic", "agenda", "participants", "notes"],
      "fields": [
        { "id": "meeting_type", "type": "select", "label": "Meeting Type", "options": ["standup", "planning", "review", "brainstorm", "one_on_one", "all_hands", "board", "external"] },
        { "id": "agenda_items", "type": "sortable-list", "label": "Agenda Items" },
        { "id": "speakers", "type": "person-list", "label": "Speakers / Presenters" },
        { "id": "required_attendees", "type": "multiselect", "label": "Required Attendees" },
        { "id": "optional_attendees", "type": "multiselect", "label": "Optional Attendees" },
        { "id": "rsvp_required", "type": "toggle", "label": "RSVP Required", "default": true },
        { "id": "meeting_minutes", "type": "rich-text", "label": "Meeting Minutes", "adminOnly": true },
        { "id": "action_items", "type": "checklist", "label": "Action Items" },
        { "id": "pre_read_materials", "type": "file", "label": "Pre-read Materials" },
        { "id": "recurring", "type": "select", "label": "Recurring Meeting", "options": ["none", "daily", "weekly", "biweekly", "monthly"] }
      ],
      "flows": ["registration", "feedback"],
      "integrations": ["calendar", "email", "slack", "teams"]
    },

    "workshop": {
      "label": "Workshop",
      "icon": "tool",
      "color": "#f59e0b",
      "description": "Hands-on learning sessions with materials and exercises",
      "defaultDuration": 4,
      "capacityDefault": 30,
      "tabs": ["basic", "curriculum", "materials", "instructor"],
      "fields": [
        { "id": "skill_level", "type": "select", "label": "Skill Level", "required": true, "options": ["beginner", "intermediate", "advanced", "all"], "showPublic": true },
        { "id": "prerequisites", "type": "sortable-list", "label": "Prerequisites", "showPublic": true },
        { "id": "learning_outcomes", "type": "sortable-list", "label": "Learning Outcomes", "showPublic": true },
        { "id": "curriculum_modules", "type": "key-value-list", "label": "Curriculum Modules", "showPublic": true },
        { "id": "hands_on_exercises", "type": "rich-text", "label": "Hands-on Exercises" },
        { "id": "materials_provided", "type": "sortable-list", "label": "Materials Provided", "showPublic": true },
        { "id": "materials_to_bring", "type": "sortable-list", "label": "Materials to Bring", "showPublic": true },
        { "id": "resource_files", "type": "file", "label": "Resource Files", "maxSize": 50 },
        { "id": "instructor_info", "type": "person-list", "label": "Instructor", "showPublic": true },
        { "id": "certification_offered", "type": "toggle", "label": "Certificate of Completion", "showPublic": true },
        { "id": "software_requirements", "type": "sortable-list", "label": "Software Requirements", "showPublic": true }
      ],
      "flows": ["registration", "certificates", "feedback"],
      "integrations": ["calendar", "email", "certificates"]
    },

    "hackathon": {
      "label": "Hackathon",
      "icon": "code",
      "color": "#8b5cf6",
      "description": "Coding competition with teams, submissions, and prizes",
      "defaultDuration": 48,
      "capacityDefault": 200,
      "tabs": ["basic", "challenge", "teams", "submissions", "judging", "prizes"],
      "fields": [
        { "id": "hackathon_format", "type": "select", "label": "Hackathon Format", "options": ["in_person", "virtual", "hybrid"], "showPublic": true },
        { "id": "duration_hours", "type": "select", "label": "Duration (Hours)", "options": ["12", "24", "36", "48", "72", "week"], "default": "48", "showPublic": true },
        { "id": "problem_statement", "type": "rich-text", "label": "Problem Statement", "required": true, "showPublic": true },
        { "id": "themes", "type": "sortable-list", "label": "Hackathon Themes/Tracks", "showPublic": true },
        { "id": "tech_requirements", "type": "multiselect", "label": "Technology Requirements", "options": ["any", "web", "mobile", "ai_ml", "blockchain", "iot", "cloud"], "showPublic": true },
        { "id": "team_size_min", "type": "number", "label": "Minimum Team Size", "default": 2, "min": 1, "max": 10, "showPublic": true },
        { "id": "team_size_max", "type": "number", "label": "Maximum Team Size", "default": 5, "min": 1, "max": 10, "showPublic": true },
        { "id": "allow_solo", "type": "toggle", "label": "Allow Solo Participants", "default": true, "showPublic": true },
        { "id": "team_formation_help", "type": "toggle", "label": "Team Formation Assistance", "default": true },
        { "id": "submission_deadline", "type": "datetime", "label": "Submission Deadline", "required": true, "showPublic": true },
        { "id": "submission_requirements", "type": "sortable-list", "label": "Submission Requirements", "showPublic": true },
        { "id": "allowed_file_types", "type": "multiselect", "label": "Allowed Submission Files", "options": ["github", "video", "presentation", "zip", "figma", "devpost"] },
        { "id": "judging_criteria", "type": "key-value-list", "label": "Judging Criteria", "description": "Criteria and weights", "showPublic": true },
        { "id": "judges", "type": "person-list", "label": "Judges", "showPublic": true },
        { "id": "judging_type", "type": "select", "label": "Judging Method", "options": ["panel", "public", "hybrid"], "default": "panel" },
        { "id": "prizes", "type": "key-value-list", "label": "Prize Categories", "showPublic": true },
        { "id": "total_prize_pool", "type": "currency", "label": "Total Prize Pool", "showPublic": true },
        { "id": "sponsors", "type": "person-list", "label": "Sponsors", "showPublic": true },
        { "id": "mentors_available", "type": "toggle", "label": "Mentors Available", "default": true, "showPublic": true },
        { "id": "mentors", "type": "person-list", "label": "Mentors", "conditional": "mentors_available", "showPublic": true },
        { "id": "rules", "type": "rich-text", "label": "Rules & Guidelines", "showPublic": true }
      ],
      "flows": ["registration", "submissions", "voting", "leaderboard", "certificates"],
      "integrations": ["calendar", "email", "slack", "discord", "devpost", "github"]
    },

    "game_jam": {
      "label": "Game Jam",
      "icon": "gamepad",
      "color": "#ec4899",
      "description": "Game development competition with themes and voting",
      "defaultDuration": 48,
      "capacityDefault": 100,
      "tabs": ["basic", "theme", "rules", "submissions", "judging"],
      "fields": [
        { "id": "jam_duration", "type": "select", "label": "Jam Duration", "options": ["24", "48", "72", "week", "month"], "default": "48", "showPublic": true },
        { "id": "theme", "type": "text", "label": "Jam Theme", "showPublic": true },
        { "id": "theme_reveal_time", "type": "datetime", "label": "Theme Reveal Time", "showPublic": true },
        { "id": "optional_themes", "type": "sortable-list", "label": "Optional/Bonus Themes" },
        { "id": "allowed_engines", "type": "multiselect", "label": "Allowed Game Engines", "options": ["any", "unity", "unreal", "godot", "gamemaker", "construct", "rpgmaker", "custom", "no_engine"], "showPublic": true },
        { "id": "asset_rules", "type": "select", "label": "Asset Rules", "options": ["all_original", "premade_allowed", "limited_premade"], "showPublic": true },
        { "id": "team_rules", "type": "select", "label": "Team Rules", "options": ["solo_only", "teams_only", "both"], "default": "both", "showPublic": true },
        { "id": "max_team_size", "type": "number", "label": "Max Team Size", "default": 4, "showPublic": true },
        { "id": "submission_platform", "type": "select", "label": "Submission Platform", "options": ["itch", "gamejolt", "custom", "github"], "default": "itch", "showPublic": true },
        { "id": "itch_jam_link", "type": "url", "label": "itch.io Jam Page", "conditional": "submission_platform:itch", "showPublic": true },
        { "id": "required_platforms", "type": "multiselect", "label": "Required Build Platforms", "options": ["web", "windows", "mac", "linux", "android", "ios"], "showPublic": true },
        { "id": "judging_categories", "type": "key-value-list", "label": "Judging Categories", "showPublic": true },
        { "id": "voting_period_days", "type": "number", "label": "Voting Period (Days)", "default": 7 },
        { "id": "community_voting", "type": "toggle", "label": "Community Voting", "default": true },
        { "id": "prizes", "type": "key-value-list", "label": "Prizes", "showPublic": true }
      ],
      "flows": ["registration", "submissions", "voting", "leaderboard"],
      "integrations": ["calendar", "email", "discord", "itch.io"]
    },

    "panel": {
      "label": "Panel Discussion",
      "icon": "message-square",
      "color": "#06b6d4",
      "description": "Moderated discussion with multiple panelists",
      "defaultDuration": 1.5,
      "capacityDefault": 100,
      "tabs": ["basic", "panelists", "topics", "audience"],
      "fields": [
        { "id": "panel_topic", "type": "text", "label": "Panel Topic", "required": true, "showPublic": true },
        { "id": "moderator", "type": "person-list", "label": "Moderator", "required": true, "showPublic": true },
        { "id": "panelists", "type": "person-list", "label": "Panelists", "required": true, "showPublic": true },
        { "id": "discussion_points", "type": "sortable-list", "label": "Discussion Points", "showPublic": true },
        { "id": "opening_question", "type": "textarea", "label": "Opening Question" },
        { "id": "audience_qa", "type": "toggle", "label": "Audience Q&A", "default": true, "showPublic": true },
        { "id": "qa_duration", "type": "number", "label": "Q&A Duration (minutes)", "default": 15, "conditional": "audience_qa" },
        { "id": "pre_submit_questions", "type": "toggle", "label": "Pre-submit Questions", "default": true },
        { "id": "live_polling", "type": "toggle", "label": "Live Polling", "default": false },
        { "id": "poll_questions", "type": "sortable-list", "label": "Poll Questions", "conditional": "live_polling" },
        { "id": "recording_available", "type": "toggle", "label": "Recording Available", "default": true, "showPublic": true }
      ],
      "flows": ["registration", "feedback"],
      "integrations": ["calendar", "email", "slido", "mentimeter"]
    },

    "fireside_chat": {
      "label": "Fireside Chat",
      "icon": "flame",
      "color": "#f97316",
      "description": "Intimate conversation between host and guest",
      "defaultDuration": 1,
      "capacityDefault": 50,
      "tabs": ["basic", "speakers", "content", "interaction"],
      "fields": [
        { "id": "chat_topic", "type": "text", "label": "Conversation Topic", "required": true, "showPublic": true },
        { "id": "host", "type": "person-list", "label": "Host / Interviewer", "required": true, "showPublic": true },
        { "id": "guest", "type": "person-list", "label": "Guest Speaker", "required": true, "showPublic": true },
        { "id": "guest_background", "type": "rich-text", "label": "Guest Background", "showPublic": true },
        { "id": "key_topics", "type": "sortable-list", "label": "Key Topics to Cover", "showPublic": true },
        { "id": "suggested_questions", "type": "sortable-list", "label": "Prepared Questions", "adminOnly": true },
        { "id": "audience_questions", "type": "toggle", "label": "Allow Audience Questions", "default": true, "showPublic": true },
        { "id": "question_moderation", "type": "toggle", "label": "Moderate Questions", "default": true, "conditional": "audience_questions" },
        { "id": "intimate_setting", "type": "toggle", "label": "Intimate Setting", "default": true },
        { "id": "networking_after", "type": "toggle", "label": "Networking Opportunity After", "default": true, "showPublic": true }
      ],
      "flows": ["registration", "feedback"],
      "integrations": ["calendar", "email"]
    },

    "demo_day": {
      "label": "Demo Day",
      "icon": "presentation",
      "color": "#22c55e",
      "description": "Startup pitch event with investor audience",
      "defaultDuration": 4,
      "capacityDefault": 150,
      "tabs": ["basic", "pitches", "investors", "feedback"],
      "fields": [
        { "id": "demo_day_type", "type": "select", "label": "Demo Day Type", "options": ["accelerator", "incubator", "competition", "internal", "open"], "showPublic": true },
        { "id": "cohort_name", "type": "text", "label": "Cohort/Batch Name", "showPublic": true },
        { "id": "pitch_duration", "type": "number", "label": "Pitch Duration (minutes)", "default": 5, "showPublic": true },
        { "id": "qa_per_pitch", "type": "number", "label": "Q&A Per Pitch (minutes)", "default": 3, "showPublic": true },
