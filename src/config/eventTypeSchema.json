        { "id": "feedback_forms", "type": "toggle", "label": "Investor Feedback Forms", "default": true },
        { "id": "meeting_scheduling", "type": "toggle", "label": "Enable Meeting Scheduling", "default": true },
        { "id": "networking_session", "type": "toggle", "label": "Networking Session", "default": true, "showPublic": true },
        { "id": "networking_duration", "type": "number", "label": "Networking Duration (minutes)", "default": 60, "conditional": "networking_session" }
